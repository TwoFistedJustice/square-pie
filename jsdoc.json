{
  "opts": {
    "template": "node_modules/docdash"
  },
  "plugins": ["plugins/markdown"],
  "docdash": {
    "commonNav": true,
    "static": true,
    "sort": true,
    "search": true,
    "collapse": true,
    "typedefs": true,
    "removeQuotes": "none",
    "wrap": true,
    "scripts": [],
    "menu": {
      "Github repo": {
        "href": "https://github.com/TwoFistedJustice/square-pie",
        "target": "_blank",
        "class": "menu-item",
        "id": "repository"
      }
    }
  },
  "templates": {
    "default": {
      "useLongnameInNav": true
    }
  },
  "source": {
    "include": ["./src"],
    "exclude": [
      "./src/lib/utilities",
      "./src/lib/enum",
      "./src/lib/regex",
      "./src/lib/temp.js"
    ]
  }
}
